class List()
class Nil() extends List
class Cons(head:Int, tail:List) extends List
let list = Cons(1,Cons(3,Cons(5,Cons(7,Nil()))))
fun sum(x:List):Int = if x is
    Cons(h,t) then h+sum(t)
    else 0
